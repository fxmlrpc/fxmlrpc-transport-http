{
    "name": "fxmlrpc/http-transport",
    "description": "HTTP Transport for fXmlRpc",
    "license": "MIT",
    "keywords": ["xmlrpc", "xml", "rpc", "http"],
    "homepage": "http://fxmlrpc.org",
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lstrojny@php.net",
            "homepage": "http://usrportage.de"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fxmlrpc/client": "dev-initial_import",
        "php-http/adapter-implementation": "0.1"
    },
    "require-dev": {
        "fxmlrpc/native-serialization": "dev-initial_import",
        "php-http/guzzle5-adapter": "dev-initial_import",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "provide": {
        "fxmlrpc/transport-implementation": "0.1"
    },
    "autoload": {
        "psr-4": {
            "fXmlRpc\\Client\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
